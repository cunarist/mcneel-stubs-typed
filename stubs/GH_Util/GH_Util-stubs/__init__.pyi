from typing import overload, Tuple, Iterable, Iterator, Sequence, MutableSequence
from enum import Enum

import MetaBall

__all__ = ['MetaBall']


class BezierF:
    @overload
    def __init__(self, p0: PointF, p1: PointF, p2: PointF, p3: PointF): ...
    @overload
    def __init__(self, x0: float, y0: float, x1: float, y1: float, x2: float, y2: float, x3: float, y3: float): ...
    @overload
    def Distance(self, point: PointF) -> float: ...
    @overload
    def Equals(self, obj: object) -> bool: ...
    @overload
    def Extremes(self) -> Tuple[Iterable[float], Iterable[float]]: ...
    @overload
    @property
    def Bounds(self) -> RectangleF: ...
    @overload
    @property
    def IsClosed(self) -> bool: ...
    @overload
    @property
    def IsEmpty(self) -> bool: ...
    @overload
    @property
    def P0(self) -> PointF: ...
    @overload
    @property
    def P1(self) -> PointF: ...
    @overload
    @property
    def P2(self) -> PointF: ...
    @overload
    @property
    def P3(self) -> PointF: ...
    @overload
    @property
    def T0(self) -> SizeF: ...
    @overload
    @property
    def T3(self) -> SizeF: ...
    @overload
    def GetHashCode(self) -> int: ...
    @overload
    def GetType(self) -> type: ...
    @overload
    def PointAt(self, t: float) -> PointF: ...
    @overload
    def Project(self, point: PointF) -> float: ...
    @overload
    def Reverse(self) -> BezierF: ...
    @overload
    def TangentAt(self, t: float) -> SizeF: ...
    @overload
    def ToString(self) -> str: ...


class BezierShape(Enum):
    Unknown = 0
    Arch = 1
    SingleInflection = 2
    DoubleInflection = 3
    Cusp = 4
    Closed = 5
    LoopAtStart = 6
    LoopAtEnd = 7
    LoopOnInterior = 8


class GH_2DIndex:
    @overload
    def __init__(self, other: GH_2DIndex): ...
    @overload
    def __init__(self, new_i: int, new_j: int): ...
    i: int
    j: int
    @overload
    def Equals(self, obj: object) -> bool: ...
    @overload
    def GetHashCode(self) -> int: ...
    @overload
    def GetType(self) -> type: ...
    @overload
    def Set(self, new_i: int, new_j: int) -> None: ...
    @overload
    def Shift(self, offset_i: int, offset_j: int) -> None: ...
    @overload
    def ToString(self) -> str: ...




